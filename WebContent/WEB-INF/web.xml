<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    id="YViewApp" version="3.0">
     
    <display-name>YView Application</display-name>
     
    <servlet>
        <servlet-name>YView</servlet-name>
        <servlet-class>xyz.yvonneshow.yview.YView</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>YView</servlet-name>
        <url-pattern>/YView</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>YImage</servlet-name>
        <servlet-class>xyz.yvonneshow.yview.YImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>YImage</servlet-name>
        <url-pattern>/YImage</url-pattern>
    </servlet-mapping>
     
</web-app>